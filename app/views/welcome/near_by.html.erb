<h2 class="text-center">Buses for <%= @stop_name %></h2>

<div class="container">
  <div class="well">
    <ul>
      <% @routes.each do |route| %>
        <li class="stopTime">
          <div class="stopTime-name">
            <%= route.short_name %>
          </div>
        </li>
      <% end %>
    </ul>
  </div>
</div>